// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL_PROD")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model WidgetSettingsData {
  id            String @id @default(uuid())
  fieldSettings Json
  fontSettings  Json
  submitButton  Json
  extraFields   Json
}
